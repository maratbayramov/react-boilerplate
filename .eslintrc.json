{
    "extends": ["eslint:recommended", "plugin:react/recommended", "google"],
    "env": {
      "node": true,
      "browser": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "linebreak-style": ["error", "windows"],
      "react/jsx-uses-vars": 1,
      "react/display-name": 1,
      "no-unused-vars": "warn",
      "no-console": 1,
      "valid-jsdoc": "off",
      "require-jsdoc": "off",
      "object-curly-spacing": "off",
      "react/prop-types": "off",
      "max-len": "off",
      "new-cap": 0
    }
}
